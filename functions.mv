	<MvASSIGN NAME = "l.dimensions" MEMBER = "scale" INDEX = "3" VALUE = "3x">
	<MvASSIGN NAME = "l.dimensions" MEMBER = "scale" INDEX = "4" VALUE = "4x">

	<MvASSIGN NAME = "l.dimensions" MEMBER = "width" INDEX = "1" VALUE = "{ l.image_width }">
