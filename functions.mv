			<MvIF EXPR = "{ l.output:srcset }">
				<MvASSIGN NAME = "l.output:srcset" VALUE = "{ l.output:srcset $ ', ' }">
			</MvIF>
			<MvASSIGN NAME = "l.output:srcset" VALUE = "{ l.output:srcset $ l.dimension:image $ ' ' $ l.dimension:scale }">
		</MvIF>
	</MvFOREACH>

</MvFUNCTION>

